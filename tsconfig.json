{
  "compilerOptions": {
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "strict": true,
    "target": "es2017",
    "lib": ["es5", "dom", "dom.iterable", "es2017"],
    "jsx": "react",
    "isolatedModules": false,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "types": ["node"]
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
